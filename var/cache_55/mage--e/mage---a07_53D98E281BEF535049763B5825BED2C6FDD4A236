<script type="text/javascript" src="http://clearcho.nextmp.net/skin/frontend/default/alamoana_theme/js/jquery.js"></script>
<script type="text/javascript">
	jQuery(function() {
		jQuery("#mini-cart").hover(function() {
			jQuery(this).addClass('hover');
			jQuery("#mini-cart .block-content").stop(true, true).delay(300).slideDown(500, "easeOutBounce");
		}, function() {
			jQuery("#mini-cart .block-content").stop(true, true).delay(300).fadeOut(800, "easeInCubic");
		});
		
		jQuery(".jewelry").hover(function() {
			jQuery(this).addClass('hover');
			jQuery(".dropdwnjew").stop(true, true).delay(300).slideDown(500, "easeOutBounce");
		}, function() {
			jQuery(".dropdwnjew").fadeOut(100, "easeInCubic");
		});

		jQuery(".baby-shop").hover(function() {
			jQuery(this).addClass('hover');
			jQuery(".drop-dwnbaby").stop(true, true).delay(300).slideDown(500, "easeOutBounce");
		}, function() {
			jQuery(".drop-dwnbaby").fadeOut(100, "easeInCubic");
		});


		jQuery(".watches").hover(function() {
			jQuery(this).addClass('hover');
			jQuery(".dropdwnwatches").stop(true, true).delay(300).slideDown(500, "easeOutBounce");
		}, function() {
			jQuery(".dropdwnwatches").fadeOut(100, "easeInCubic");
		});

		jQuery(".fashion").hover(function() {
			jQuery(this).addClass('hover');
			jQuery(".dropdwnfashion").stop(true, true).delay(300).slideDown(500, "easeOutBounce");
		}, function() {
			jQuery(".dropdwnfashion").fadeOut(100, "easeInCubic");
		});
		
		jQuery(".languagepack").hover(function() {
			jQuery(this).addClass('hover');
			jQuery(".lng-box").stop(true, true).delay(300).slideDown(500, "easeOutBounce");
		}, function() {
			jQuery(".lng-box").fadeOut(100, "easeInCubic");
		});
		
		jQuery("#back-top").hide();
		jQuery(window).scroll(function () {
			if (jQuery(this).scrollTop() > 100) {
				jQuery('#back-top').fadeIn();
			} else {
				jQuery('#back-top').fadeOut();
			}
		});
		jQuery('#back-top a').click(function () {
			jQuery('body,html').animate({
				scrollTop: 0
			}, 800);
			return false;
		});

	});
</script>


